<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:content="http://purl.org/rss/1.0/modules/content/" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:media="http://search.yahoo.com/mrss/" xmlns:atom="http://www.w3.org/2005/Atom" xmlns:georss="http://www.georss.org/georss">
  <channel>
    <title><![CDATA[ {{website_title}} ]]></title>
    <link>{{website_url}}</link>
    <description><![CDATA[ {{website_description}} ]]></description>
    <atom:link href="{{fd_rss_feed_url}}" rel="self" type="application/rss+xml" />
    {{for post in rss_posts}}
      <item>
        <title><![CDATA[ {{post.title}} ]]></title>
        
        <link>{{website_url}}{{spy post.link}}</link>
        <guid>{{website_url}}{{spy post.link}}</guid>
        
        <description><![CDATA[ {{post.description}} ]]></description>
        <pubDate>{{post.date}}</pubDate>
        <atom:author>
          <atom:name>{{author}}</atom:name>
        </atom:author>
      </item>
    {{endfor}}
  </channel>
</rss>